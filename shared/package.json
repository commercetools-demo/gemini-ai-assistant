{
  "name": "@commercetools-demo/ai-assistant",
  "version": "1.0.0",
  "description": "AI Assistant for the commercetools platform",
  "private": true,
  "keywords": [
    "javascript",
    "frontend",
    "commercetools",
    "react",
    "ai"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "manypkg exec rm -rf dist",
    "build": "yarn preconstruct build",
    "build:watch": "yarn preconstruct watch",
    "start": "cd test-application && yarn start",
    "lint": "jest --config jest.eslint.config.js",
    "format": "prettier --write '**/*.{js,ts,tsx}'"
  },
  "workspaces": [
    "test-application",
    "packages/*"
  ],
  "preconstruct": {
    "packages": [
      "packages/*"
    ],
    "externals": [
      "react",
      "react-dom",
      "react-intl",
      "styled-components"
    ]
  },
  "engines": {
    "node": ">=22",
    "npm": ">=6"
  },
  "packageManager": "yarn@4.5.1",
  "resolutions": {
    "form-data": "^3.0.4",
    "loader-utils": "^2.0.3",
    "nth-check": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commercetools-frontend/eslint-config-mc-app": "24.0.0",
    "@manypkg/cli": "0.24.0",
    "@preconstruct/cli": "2.8.10",
    "@types/add": "^2",
    "@types/babel__preset-env": "^7",
    "@types/node": "^22.13.1",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "8.18.0",
    "@typescript-eslint/parser": "8.18.0",
    "babel-jest": "29.7.0",
    "babel-loader": "9.2.1",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-typescript-to-proptypes": "1.4.2",
    "eslint": "8.57.1",
    "eslint-formatter-pretty": "4.1.0",
    "jest": "29.7.0",
    "jest-runner-eslint": "1.2.0",
    "jest-validate": "29.7.0",
    "jest-watch-typeahead": "2.2.2",
    "prettier": "2.8.8",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-intl": "^7.1.4",
    "react-router-dom": "5.3.4",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.2"
  }
}
